{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "cacheDir": ".turbo/cache",
  "envMode": "strict",
  "globalDependencies": ["tsconfig.json", "package.json", "bun.lockb", "turbo.json", ".nvmrc"],
  "globalEnv": ["CI", "NODE_ENV"],
  "globalPassThroughEnv": [
    "TURBO_TOKEN",
    "TURBO_TEAM",
    "TURBO_REMOTE_ONLY",
    "CONVEX_DEPLOYMENT",
    "GITHUB_TOKEN"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.local",
        ".env.production",
        ".env.staging",
        ".env*.example"
      ],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "src-tauri/target/release/**"],
      "env": [
        "NEXT_PUBLIC_*",
        "VITE_*",
        "GITHUB_CLIENT_ID",
        "GITHUB_CLIENT_SECRET",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "BETTER_AUTH_SECRET",
        "TAURI_SIGNING_PRIVATE_KEY",
        "TAURI_SIGNING_PUBLIC_KEY"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.ts",
        "**/*.tsx",
        "**/*.js",
        "**/*.jsx",
        "!node_modules/**",
        "!.next/**",
        "!dist/**"
      ],
      "outputs": [],
      "env": ["DEV", "EMAIL_FROM", "RESEND_API_KEY", "SKIP_ENV_VALIDATION", "MODE"]
    },
    "check-types": {
      "dependsOn": ["^check-types"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.ts",
        "**/*.tsx",
        "**/*.d.ts",
        "tsconfig.json",
        "!node_modules/**",
        "!.next/**",
        "!dist/**"
      ],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "NEXT_PUBLIC_*",
        "VITE_*",
        "GITHUB_CLIENT_ID",
        "GITHUB_CLIENT_SECRET",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "BETTER_AUTH_SECRET"
      ]
    },
    "dev:vite": {
      "cache": false,
      "persistent": true,
      "env": ["VITE_*", "DEV"]
    },
    "tauri": {
      "cache": false,
      "persistent": true,
      "env": ["TAURI_*", "VITE_*"]
    },
    "tauri:build": {
      "dependsOn": ["^build"],
      "outputs": ["src-tauri/target/release/**", "src-tauri/target/debug/**"],
      "env": [
        "TAURI_SIGNING_PRIVATE_KEY",
        "TAURI_SIGNING_PUBLIC_KEY",
        "APPLE_SIGNING_CERTIFICATE",
        "APPLE_SIGNING_PASSWORD",
        "WINDOWS_SIGNING_CERTIFICATE",
        "WINDOWS_SIGNING_PASSWORD"
      ]
    },
    "tauri:dev": {
      "cache": false,
      "persistent": true,
      "env": ["TAURI_*", "VITE_*"]
    },
    "shadcn": {
      "cache": false,
      "interactive": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"],
      "env": ["NODE_ENV"]
    },
    "//#format": {
      "cache": false,
      "outputs": []
    },
    "//#format:check": {
      "outputs": []
    },
    "//#cleanup-ports": {
      "cache": false
    },
    "//#ci:test": {
      "dependsOn": ["lint", "check-types", "//#format:check"],
      "cache": false
    },
    "//#ci:build": {
      "dependsOn": ["build"]
    },
    "//#precommit": {
      "dependsOn": ["//#ci:test"]
    },
    "//#changeset": {
      "cache": false,
      "interactive": true
    },
    "//#changeset:version": {
      "cache": false,
      "outputs": [".changeset/**", "**/CHANGELOG.md", "**/package.json"]
    },
    "//#changeset:publish": {
      "cache": false
    },
    "//#release": {
      "cache": false,
      "dependsOn": ["//#changeset:version", "//#changeset:publish"]
    },
    "@truss/web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [
        "NEXT_PUBLIC_*",
        "GITHUB_CLIENT_ID",
        "GITHUB_CLIENT_SECRET",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "BETTER_AUTH_SECRET",
        "EMAIL_FROM",
        "RESEND_API_KEY",
        "SKIP_ENV_VALIDATION"
      ]
    },
    "@truss/web#lint": {
      "dependsOn": ["^lint"],
      "env": ["EMAIL_FROM", "RESEND_API_KEY", "SKIP_ENV_VALIDATION"]
    },
    "@truss/precision#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "src-tauri/target/release/**"],
      "env": ["VITE_*", "TAURI_SIGNING_PRIVATE_KEY", "TAURI_SIGNING_PUBLIC_KEY"]
    },
    "@truss/momentum#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "src-tauri/target/release/**"],
      "env": ["VITE_*", "TAURI_SIGNING_PRIVATE_KEY", "TAURI_SIGNING_PUBLIC_KEY"]
    },
    "@truss/ui#build": {
      "outputs": [],
      "cache": true
    },
    "@truss/types#build": {
      "outputs": [],
      "cache": true
    },
    "@truss/types#lint": {
      "cache": true
    },
    "@truss/types#check-types": {
      "cache": true
    },
    "@truss/lib#build": {
      "outputs": [],
      "cache": true
    },
    "@truss/lib#lint": {
      "cache": true
    },
    "@truss/lib#check-types": {
      "cache": true
    },
    "@truss/config#build": {
      "outputs": [],
      "cache": true
    },
    "@truss/config#lint": {
      "cache": true
    },
    "@truss/config#check-types": {
      "cache": true
    },
    "@truss/features#build": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": true
    },
    "@truss/features#lint": {
      "dependsOn": ["^lint"],
      "cache": true
    },
    "@truss/features#check-types": {
      "dependsOn": ["^check-types"],
      "cache": true
    },
    "@truss/backend#dev": {
      "cache": false,
      "persistent": true
    },
    "@truss/backend#check-types": {
      "cache": true
    },
    "@truss/auth#build": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": true
    },
    "@truss/auth#lint": {
      "dependsOn": ["^lint"],
      "cache": true
    },
    "@truss/auth#check-types": {
      "dependsOn": ["^check-types"],
      "cache": true
    }
  },
  "remoteCache": {}
}
